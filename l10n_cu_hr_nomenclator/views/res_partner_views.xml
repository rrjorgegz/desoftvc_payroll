<?xml version="1.0" encoding="utf-8"?>
<odoo>

        <record model="ir.ui.view" id="l10n_cu_hr_nomenclator_view_partner_view_form">
            <field name="name">l10n_cu_hr_nomenclator.view_partner.form</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form" />
            <field name="priority" eval="50"/>
            <field name="arch" type="xml">
                <field name="street" position="before">
                    <field name="agency_ok"/>
                </field>
                <field name="city" position="after">
                    <field name="municipality" class="o_address_state" placeholder="Municipio" options='{"no_open": True, "no_create": True}'/>
                </field>
            </field>
        </record>
        
</odoo>