<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="view_generar_licencia_wiz_form" model="ir.ui.view">
            <field name="name">comercializador.generar.licencia.wiz.form</field>
            <field name="model">comercializador.generar.licencia.wiz</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group col="3">
                            <field name="semilla" required="1"/>
                            <field name="fecha_fin_subscripcion" />
                            <field name="licencia_generada" readonly="1" widget="CopyClipboardChar" />
                        </group>
                    </sheet>
                    <footer>
                        <button name="button_generar_licencia" string="Generar" type="object"
                            class="btn-primary" />
                        <button string="Cancelar" class="oe_link" special="cancel" />
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>