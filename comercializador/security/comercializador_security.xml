<?xml version="1.0" ?>
<odoo>
    <data>


    <record id="categoria_modulo_comercializador" model="ir.module.category">
        <field name="name">Comercializador</field>
    </record>

    <record id="comercializador_group_user" model="res.groups">
        <field name="name">Usuario</field>
        <field name="category_id" ref="categoria_modulo_comercializador"/>
        <field name="implied_ids" eval="[(4,ref('base.group_user'))]"/>
    </record>

    <record id="comercializador_group_gestor" model="res.groups">
        <field name="name">Gestor</field>
        <field name="category_id" ref="categoria_modulo_comercializador"/>
        <field name="implied_ids" eval="[(4,ref('comercializador_group_user'))]"/>
        <field name="users" eval="[(4,ref('base.user_root')),(4,ref('base.user_admin'))]"/>
    </record>
</data>
</odoo>